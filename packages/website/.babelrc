{
  "extends": "../../.babelrc",
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"]
      }
    ],
    [
      "inline-react-svg",
      {
        "svgo": {
          "plugins": [
            {
              "cleanupIDs": true,
              "removeViewBox": false
            }
          ]
        }
      }
    ]
  ],
  "presets": ["next/babel"]
}
